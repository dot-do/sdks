  },
  "keywords": [
    "rpc",
    "proxy",
    "pipelining",
    "websocket",
    "dotdo"
  ],
  "author": "DotDo",
  "license": "MIT",
  "dependencies": {
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "js-yaml": "^4.1.0",
    "@types/js-yaml": "^4.0.9",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  }
}
