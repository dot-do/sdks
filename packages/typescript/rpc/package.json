{
  "name": "@dotdo/rpc",
  "version": "0.1.0",
  "description": "DotDo RPC Client - Type-safe remote procedure calls",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
