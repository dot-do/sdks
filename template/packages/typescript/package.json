{
  "name": "{{name}}.do",
  "version": "0.1.0",
  "description": "{{Name}}.do SDK - {{description}}",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "conformance": "vitest run --config vitest.conformance.config.ts"
  },
  "keywords": [
    "{{name}}",
    "rpc",
    "dotdo",
    "sdk"
  ],
  "author": "DotDo",
  "license": "MIT",
  "dependencies": {
    "rpc.do": "^0.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-do/{{name}}"
  },
  "homepage": "https://{{name}}.do"
}
